    },
    "./*": "./*"
  },
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "serve": "vite preview"
  },
  "devDependencies": {
    "@antfu/eslint-config": "latest",
    "@cloydlau/scripts": "latest",
    "case-police": "latest",
    "husky": "latest",
    "vite": "latest"
  }
}