{
  "name": "webgl-fluid",
  "version": "0.0.4",
  "description": "ES module support for https://github.com/PavelDoGreat/WebGL-Fluid-Simulation",
  "private": false,
  "license": "MIT",
  "main": "./dist/webgl-fluid.umd.js",
  "module": "./dist/webgl-fluid.es.js",
  "exports": {
    ".": {
      "import": "./dist/webgl-fluid.es.js",
      "require": "./dist/webgl-fluid.umd.js"
    }
  },
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "serve": "vite preview",
    "clean": "rd /s /q .\\node_modules",
    "ri": "npm run clean && pnpm i",
    "u": "node scripts/upgrade.js",
    "r": "node scripts/release.js",
    "rs": "node scripts/release.js --straightforward"
  },
  "dependencies": {},
  "devDependencies": {
    "vite": "latest"
  }
}
